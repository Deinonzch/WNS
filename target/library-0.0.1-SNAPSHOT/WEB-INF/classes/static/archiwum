--app.js

'use strict';

angular.module('bookStore', []).config(function ($routeProvider, $locationProvider) {
    $locationProvider.html5Mode(true);

    $routeProvider.
        when("/", {templateURL: "index.html"}).
        when("/about", {templateURL: "html/about.html"})
}).
    run(function () {

    });


